{
    "name": "networkrailbusinesssystems/user-login",
    "description": "Provide user login with active directory account with minimal overhead",
    "type": "library",
    "homepage": "https://github.com/Network-Rail-Business-Systems/user-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Ambreen Jamal",
            "email": "ambreen.jamal@hormail.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8",
        "illuminate/support": "^12",
        "anthonyedmonds/govuk-laravel": "^5"
    },
    "require-dev": {
        "laravel/pint": "^1",
        "orchestra/testbench": "^10",
        "phpunit/phpunit": "^12",
        "brianium/paratest": "^7",
        "larastan/larastan": "^3",
        "laracasts/flash": "^3",
        "networkrailbusinesssystems/badges": "^1"
    },
    "autoload": {
        "psr-4": {
            "NetworkRailBusinessSystems\\UserLogin\\": "src/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetworkRailBusinessSystems\\UserLogin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetworkRailBusinessSystems\\UserLogin\\Providers\\UserLoginServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
